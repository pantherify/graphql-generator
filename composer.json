    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "doctrine/dbal": "^2.9"
    },
    "authors": [
        {
            "name": "David Fernandes",
            "email": "davidsf@pantherify.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pantherify\\GraphQLGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pantherify\\GraphQLGenerator\\GraphQLGeneratorServiceProvider"
            ]
        }
    }
}
